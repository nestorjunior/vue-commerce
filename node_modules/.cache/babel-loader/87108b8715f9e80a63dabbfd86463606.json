div class=\"home\">\n    <div class=\"products\">\n      <template v-for=\"(product, index) in this.products\" :key=\"index\">\n        <router-link :to=\"'/product/' + product.id\">\n          <div class=\"product\">\n            <div class=\"product-image\" :style=\"{ backgroundImage: 'url(' + product.image + ')' }\">\n\n            </div>\n            <h4>{{product.title}}</h4>\n            <p class=\"price\">$ {{product.price}}</p>\n            <button @click.stop.prevent=\"addToBag(product)\">Add to cart</button>\n          </div>\n        </router-link>\n      </template>\n      \n    </div>\n    \n  </div>\n</template>\n\n<script>\n  import { mapState } from 'vuex';\n\nexport default {\n  name: 'Home',\n\n  methods: {\n    addToBag(product) {\n      this.$store.dispatch('addToBag',product);\n    }\n  },\n  computed: mapState([\n        'products',\n        'productsInBag'\n    ]),\n}\n</script>\n\n<style lang=\"scss\">\n\n  .home {\n\n    .products {\n      display: flex;\n      flex-wrap: wrap;\n\n      a {\n        text-decoration: none;\n        color: black;\n        box-sizing: border-box;        \n        flex: 0 0 33.333333%;\n      }\n\n\n      .product {\n        box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\n        padding: 16px;\n        margin: 8px;\n        height: 300px;\n        \n        .product-image {\n          margin: 20px auto;\n          width: 160px;\n          height: 140px;\n          background-size: contain;\n          background-position: center;\n          background-repeat: no-repeat;\n        }\n        h4 {\n          margin-top: 0;\n          font-size: 12px;\n        }\n\n        p.price {\n          font-size: 20px;\n          font-weight: bold;\n        }\n\n        button {\n          color: #fff;\n          background-color: #007bff;\n          border: 1px solid #007bff;\n          font-weight: 400;\n          text-align: center;\n          padding: 8px;\n          cursor: pointer;\n\n          &:hover {\n            opacity: 0.8;\n          }\n        }\n      }\n    }\n\n  }\n\n</style>\n"],"sourceRoot":""}]}